<clixon-config xmlns="http://clicon.org/config">
  <CLICON_CONFIGFILE>@SYSCONFDIR@/clixon/3gpp.xml</CLICON_CONFIGFILE>
  <CLICON_CONFIGDIR>@SYSCONFDIR@/clixon/3gpp</CLICON_CONFIGDIR>
  <CLICON_FEATURE>*:*</CLICON_FEATURE>
  <CLICON_YANG_DIR>@DATADIR@/clixon</CLICON_YANG_DIR>
  <CLICON_YANG_MAIN_DIR>@DATADIR@/clixon/3gpp</CLICON_YANG_MAIN_DIR>
  <CLICON_CLI_MODE>3gpp</CLICON_CLI_MODE>
  <CLICON_CLISPEC_DIR>@LIBEXECDIR@/3gpp/clispec</CLICON_CLISPEC_DIR>
  <CLICON_SOCK>@LOCALSTATEDIR@/3gpp.sock</CLICON_SOCK>
  <CLICON_BACKEND_PIDFILE>@LOCALSTATEDIR@/3gpp.pidfile</CLICON_BACKEND_PIDFILE>
  <CLICON_XMLDB_DIR>@LOCALSTATEDIR@/3gpp</CLICON_XMLDB_DIR>
  <CLICON_STARTUP_MODE>init</CLICON_STARTUP_MODE>
</clixon-config>
