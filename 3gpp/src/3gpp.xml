<clixon-config xmlns="http://clicon.org/config">
  <CLICON_CONFIGFILE>/usr/local/etc/3gpp.xml</CLICON_CONFIGFILE>
  <CLICON_FEATURE>*:*</CLICON_FEATURE>
  <CLICON_YANG_DIR>/usr/local/share/clixon</CLICON_YANG_DIR>
  <CLICON_YANG_MAIN_DIR>/usr/local/share/clixon/3gpp</CLICON_YANG_MAIN_DIR>
  <CLICON_CLI_MODE>3gpp</CLICON_CLI_MODE>
  <CLICON_CLISPEC_DIR>/usr/local/lib/3gpp/clispec</CLICON_CLISPEC_DIR>
  <CLICON_SOCK>/usr/local/var/3gpp.sock</CLICON_SOCK>
  <CLICON_BACKEND_PIDFILE>/usr/local/var/3gpp.pidfile</CLICON_BACKEND_PIDFILE>
  <CLICON_XMLDB_DIR>/usr/local/var/3gpp</CLICON_XMLDB_DIR>
  <CLICON_STARTUP_MODE>init</CLICON_STARTUP_MODE>
</clixon-config>
