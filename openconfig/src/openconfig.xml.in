<clixon-config xmlns="http://clicon.org/config">
  <CLICON_CONFIGFILE>@SYSCONFDIR@/clixon/openconfig.xml</CLICON_CONFIGFILE>
  <CLICON_CONFIGDIR>@SYSCONFDIR@/clixon/openconfig</CLICON_CONFIGDIR>
  <CLICON_FEATURE>ietf-netconf:startup</CLICON_FEATURE>
  <CLICON_FEATURE>clixon-restconf:allow-auth-none</CLICON_FEATURE>
  <CLICON_YANG_DIR>@DATADIR@/clixon</CLICON_YANG_DIR>
  <CLICON_YANG_DIR>@DATADIR@/openconfig/public/release/models</CLICON_YANG_DIR>
  <CLICON_YANG_MODULE_MAIN>openconfig-system</CLICON_YANG_MODULE_MAIN>
  <CLICON_CLI_MODE>openconfig</CLICON_CLI_MODE>
  <CLICON_CLISPEC_DIR>@LIBDIR@/openconfig/clispec</CLICON_CLISPEC_DIR>
  <CLICON_SOCK>@LOCALSTATEDIR@/openconfig/openconfig.sock</CLICON_SOCK>
  <CLICON_BACKEND_PIDFILE>@LOCALSTATEDIR@/openconfig/openconfig.pidfile</CLICON_BACKEND_PIDFILE>
  <!-- true here means restconf config is in datastore -->
  <CLICON_BACKEND_RESTCONF_PROCESS>true</CLICON_BACKEND_RESTCONF_PROCESS>
  <CLICON_XMLDB_DIR>@LOCALSTATEDIR@/openconfig</CLICON_XMLDB_DIR>
  <CLICON_STARTUP_MODE>init</CLICON_STARTUP_MODE>
</clixon-config>
