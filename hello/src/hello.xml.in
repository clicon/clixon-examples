<clixon-config xmlns="http://clicon.org/config">
  <CLICON_CONFIGFILE>@SYSCONFDIR@/clixon/hello.xml</CLICON_CONFIGFILE>
  <CLICON_CONFIGDIR>@SYSCONFDIR@/clixon/hello</CLICON_CONFIGDIR>
  <CLICON_FEATURE>ietf-netconf:startup</CLICON_FEATURE>
  <CLICON_FEATURE>clixon-restconf:allow-auth-none</CLICON_FEATURE>
  <CLICON_YANG_DIR>@DATADIR@/clixon</CLICON_YANG_DIR>
  <CLICON_YANG_MODULE_MAIN>clixon-hello</CLICON_YANG_MODULE_MAIN>
  <CLICON_CLI_MODE>hello</CLICON_CLI_MODE>
  <CLICON_CLISPEC_DIR>@LIBEXECDIR@/hello/clispec</CLICON_CLISPEC_DIR>
  <CLICON_SOCK>@LOCALSTATEDIR@/hello.sock</CLICON_SOCK>
  <CLICON_BACKEND_PIDFILE>@LOCALSTATEDIR@/hello.pidfile</CLICON_BACKEND_PIDFILE>
  <CLICON_BACKEND_RESTCONF_PROCESS>true</CLICON_BACKEND_RESTCONF_PROCESS>
  <CLICON_XMLDB_DIR>@LOCALSTATEDIR@/hello</CLICON_XMLDB_DIR>
  <CLICON_STARTUP_MODE>init</CLICON_STARTUP_MODE>
  <CLICON_SOCK_GROUP>@CLICON_GROUP@</CLICON_SOCK_GROUP>
  <CLICON_NETCONF_HELLO_OPTIONAL>true</CLICON_NETCONF_HELLO_OPTIONAL>
  <CLICON_RESTCONF_USER>@CLICON_USER@</CLICON_RESTCONF_USER>
  <CLICON_RESTCONF_PRIVILEGES>drop_perm</CLICON_RESTCONF_PRIVILEGES>
  <CLICON_RESTCONF_INSTALLDIR>@SBINDIR@</CLICON_RESTCONF_INSTALLDIR>
</clixon-config>
