<clixon-config xmlns="http://clicon.org/config">
  <CLICON_CONFIGFILE>/usr/local/etc/wifi.xml</CLICON_CONFIGFILE>
  <CLICON_FEATURE>ietf-netconf:startup</CLICON_FEATURE>
  <CLICON_FEATURE>clixon-restconf:allow-auth-none</CLICON_FEATURE>
  <CLICON_YANG_DIR>/usr/local/share/clixon</CLICON_YANG_DIR>
  <CLICON_YANG_DIR>/usr/local/share/openconfig/public/release/models</CLICON_YANG_DIR>
  <CLICON_YANG_DIR>/usr/local/share/yang/standard</CLICON_YANG_DIR>
  <CLICON_YANG_MODULE_MAIN>clixon-wifi</CLICON_YANG_MODULE_MAIN>
  <CLICON_CLI_MODE>wifi</CLICON_CLI_MODE>
  <CLICON_CLI_LINESCROLLING>0</CLICON_CLI_LINESCROLLING>
  <CLICON_CLI_TAB_MODE>0</CLICON_CLI_TAB_MODE>
  <CLICON_CLISPEC_DIR>/usr/local/lib/wifi/clispec</CLICON_CLISPEC_DIR>
  <CLICON_BACKEND_DIR>/usr/local/lib/wifi/backend</CLICON_BACKEND_DIR>
  <CLICON_SOCK>/usr/local/var/wifi.sock</CLICON_SOCK>
  <CLICON_BACKEND_PIDFILE>/usr/local/var/wifi.pidfile</CLICON_BACKEND_PIDFILE>
  <CLICON_BACKEND_RESTCONF_PROCESS>true</CLICON_BACKEND_RESTCONF_PROCESS>
  <CLICON_XMLDB_DIR>/usr/local/var/wifi</CLICON_XMLDB_DIR>
  <CLICON_STARTUP_MODE>init</CLICON_STARTUP_MODE>
  <CLICON_SOCK_GROUP>clicon</CLICON_SOCK_GROUP>
  <CLICON_RESTCONF_USER>clicon</CLICON_RESTCONF_USER>
  <CLICON_RESTCONF_PRIVILEGES>drop_perm</CLICON_RESTCONF_PRIVILEGES>
  <CLICON_RESTCONF_INSTALLDIR>/usr/local/sbin</CLICON_RESTCONF_INSTALLDIR>
  <autocli>
     <module-default>false</module-default>
     <list-keyword-default>kw-nokey</list-keyword-default>
     <treeref-state-default>true</treeref-state-default>
     <rule>
        <name>include openconfig</name>
        <operation>enable</operation>
        <module-name>openconfig*</module-name>
     </rule>
     <rule>
        <name>compress</name>
        <operation>compress</operation>
	<yang-keyword>container</yang-keyword>
	<yang-keyword-child>list</yang-keyword-child>
     </rule>
     <rule>
        <name>openconfig compress</name>
	<operation>compress</operation>
	<yang-keyword>container</yang-keyword>
	<schema-nodeid>config</schema-nodeid>
	<module-name>openconfig*</module-name>
     </rule>
  </autocli>
</clixon-config>
