<clixon-config xmlns="http://clicon.org/config">
  <CLICON_CONFIGFILE>@SYSCONFDIR@/clixon/wifi.xml</CLICON_CONFIGFILE>
  <CLICON_CONFIGDIR>@SYSCONFDIR@/clixon/wifi</CLICON_CONFIGDIR>
  <CLICON_FEATURE>ietf-netconf:startup</CLICON_FEATURE>
  <CLICON_FEATURE>clixon-restconf:allow-auth-none</CLICON_FEATURE>
  <CLICON_YANG_DIR>@DATADIR@/clixon</CLICON_YANG_DIR>
  <CLICON_YANG_DIR>@DATADIR@/openconfig/public/release/models</CLICON_YANG_DIR>
  <CLICON_YANG_DIR>@DATADIR@/yang/standard</CLICON_YANG_DIR>
  <CLICON_YANG_MODULE_MAIN>clixon-wifi</CLICON_YANG_MODULE_MAIN>
  <CLICON_CLI_MODE>wifi</CLICON_CLI_MODE>
  <CLICON_CLI_LINESCROLLING>0</CLICON_CLI_LINESCROLLING>
  <CLICON_CLI_TAB_MODE>0</CLICON_CLI_TAB_MODE>
  <CLICON_CLISPEC_DIR>@LIBEXECDIR@/wifi/clispec</CLICON_CLISPEC_DIR>
  <CLICON_BACKEND_DIR>@LIBEXECDIR@/wifi/backend</CLICON_BACKEND_DIR>
  <CLICON_SOCK>@LOCALSTATEDIR@/wifi.sock</CLICON_SOCK>
  <CLICON_BACKEND_PIDFILE>@LOCALSTATEDIR@/wifi.pidfile</CLICON_BACKEND_PIDFILE>
  <CLICON_BACKEND_RESTCONF_PROCESS>true</CLICON_BACKEND_RESTCONF_PROCESS>
  <CLICON_XMLDB_DIR>@LOCALSTATEDIR@/wifi</CLICON_XMLDB_DIR>
  <CLICON_STARTUP_MODE>init</CLICON_STARTUP_MODE>
  <CLICON_SOCK_GROUP>@CLICON_GROUP@</CLICON_SOCK_GROUP>
  <CLICON_RESTCONF_USER>@CLICON_USER@</CLICON_RESTCONF_USER>
  <CLICON_RESTCONF_PRIVILEGES>drop_perm</CLICON_RESTCONF_PRIVILEGES>
  <CLICON_RESTCONF_INSTALLDIR>@SBINDIR@</CLICON_RESTCONF_INSTALLDIR>
</clixon-config>
